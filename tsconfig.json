{
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": false,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": [
      "esnext",
      "dom"
    ],
    //===========编译器ide提示。。。。
    "baseUrl": "./",
    "paths": {
      "/@/*": [
        "./src/*"
      ]
    },
    //===========编译器ide提示。。。。
    "types": [
      "vite-plugin-svg-icons/client"
    ]
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "auto-imports.d.ts"
  ]
}
